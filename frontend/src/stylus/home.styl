.circlepross .circle {
  width: 1.52rem;
  height: 1.52rem;
  display: inline-block !important;
  position: relative;
  border:1px solid #ddd;
  background-image url('/static/img/circle.png')
  background-size 100% 100%;
  display inline-block !important
}

.circlepross .pie_left, .circlepross .pie_right {
  width: 1.52rem
  height: 1.52rem
  position: absolute;
  top: 0;left: 0;
}
.circlepross .left, .circlepross .right {
  display: block;
  width:1.52rem
  height:1.52rem
  border 0.18rem solid #fff
  border-radius: 50%;
  position: absolute;
  top: 0;
  -webkit-transform: rotate(45deg);
}
.circleProgress_wrapper .rightcircle{
  border-top:0.18rem solid transparent
  border-right:0.18rem solid transparent
  right:0;
  -webkit-animation: circleProgressLoad_right 5s linear infinite;
}
.circleProgress_wrapper .leftcircle{
  border-bottom:0.18rem solid transparent
  border-left:0.18rem solid transparent
  left: 0;
  transform: rotate(30deg);
}
.circlepross .pie_right, .circlepross .right {
  clip:rect(0,auto,auto,0.76rem);
}
.circlepross .pie_left, .circlepross .left {
  clip:rect(0,0.76rem,auto,0);
}
.circlepross .mask {
  width: 1.02rem;
  height: 1.02rem;
  border-radius: 50%;
  left: 0.25rem;
  top: 0.25rem;
  background: #FFF;
  position: absolute;
  text-align: center;
  line-height: 1.02rem;
  font-size: 0.3rem
}
