.permission_set
    margin-bottom -15px
    clearfix()
    >ul>li
        float left
        padding 10px 0
        width 162px
        height 280px
        overflow auto
        position relative
        border-right 1px dashed #ccc
        opacity 0.8
        &:nth-child(6n)
            border-right 0
        &:nth-child(1)
        &:nth-child(2)
        &:nth-child(3)
        &:nth-child(4)
        &:nth-child(5)
        &:nth-child(6)
